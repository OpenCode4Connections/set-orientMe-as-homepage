{
  "name": "smoothLoad",
  "title": "smoothLoad",
  "description": "Hides UI until page has fully loaded, giving a smoother loading experience",
  "services": ["Customizer"],
  "extensions": [
    {
      "name": "Smooth Loader",
      "type": "com.ibm.customizer.ui",
      "payload": {
        "include-files": ["smoothLoad/smoothLoad.js", "smoothLoad/smoothLoad.css"]
      },
      "path": "global"
    }
  ]
}
